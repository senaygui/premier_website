              <div class="col-md-12 col-lg-12 col-xl-12 mb-4">
                <%= f.input :fullname, class: "form-control"%>
                <div class="validate"></div>
              </div>
              <div class="col-md-12 col-lg-12 col-xl-12 mb-4">
                <%= f.input :category, collection: ["testimonial", "comment", "review"], class: "form-control"%>
